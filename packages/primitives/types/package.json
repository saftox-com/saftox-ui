{
  "name": "@saftox-ui/types",
  "author": "Serhii Zaitsev <ceo@saftox.com>",
  "version": "0.0.1",
  "description": "A collection of low level user types primitives.",
  "keywords": ["types"],
  "main": "./src/index.ts",
  "sideEffects": false,
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saftox-com/saftox-ui.git",
    "directory": "packages/primitives/types"
  },
  "bugs": {
    "url": "https://github.com/saftox-com/saftox-ui/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "bun build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "solid-js": ">=1.8.17"
  },
  "dependencies": {
    "@solid-primitives/platform": "^0.1.2",
    "@solid-primitives/props": "^3.1.11",
    "@solid-primitives/utils": "^6.2.3"
  },
  "devDependencies": {
    "solid-js": "^1.8.17"
  },
  "clean-package": "../../../configs/clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": ["cjs", "esm"]
  },
  "primitive": {
    "name": "types",
    "stage": 0,
    "list": [],
    "category": ""
  }
}
