{
  "name": "@saftox-ui/i18n",
  "author": "Serhii Zaitsev <ceo@saftox.com>",
  "version": "0.0.1",
  "description": "Primitives for dealing with locale and layout direction.",
  "keywords": ["i18n"],
  "main": "./src/index.ts",
  "sideEffects": false,
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saftox-com/saftox-ui.git",
    "directory": "packages/primitives/i18n"
  },
  "bugs": {
    "url": "https://github.com/saftox-com/saftox-ui/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "dev:vite": "vite serve dev",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "solid-js": ">=1.8.17"
  },
  "dependencies": {
    "@internationalized/number": "^3.5.2",
    "@solid-primitives/utils": "^6.2.3",
    "@saftox-ui/utils": "0.0.1",
    "@saftox-ui/types": "0.0.1"
  },
  "devDependencies": {
    "solid-js": "^1.8.17"
  },
  "clean-package": "../../../configs/clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": ["cjs", "esm"]
  },
  "primitive": {
    "name": "i18n",
    "stage": 0,
    "list": [],
    "category": ""
  }
}
